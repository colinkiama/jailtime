{
  "name": "Jailtime",
  "description": "Blocks Websites. Keeps you focused.",
  "version": "1.0.3",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "fullstop@colinkiama.com",
      "strict_min_version": "60.0"
    }
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "/images/jailtime-32.png",
    "default_popup": "views/popup.html",
    "default_title": "Jailtime"
  },
  "permissions": ["storage", "tabs"],
  "options_ui": {
    "page": "views/settings/blocklist.html",
    "browser_style": false,
    "chrome_style": false,
    "open_in_tab": true
  },
  "icons":  {
    "16": "/images/jailtime-16.png",
    "32": "/images/jailtime-32.png",
    "48": "/images/jailtime-48.png",
    "128": "/images/jailtime-128.png"
  }
}