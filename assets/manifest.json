{
  "name": "jailtime",
  "description": "Blocks Websites. Keeps you focused.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "views/popup.html"
  },
  "permissions": ["storage", "tabs"],
  "options_page": "views/settings/blocklist.html" 
}